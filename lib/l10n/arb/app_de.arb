    "basicPlaceholderTitle": "Einfacher Platzhaltertext",
    "basicPlaceholderContent": "Hallo {name}, Sie sind {age} Jahre alt und haben {experience} Jahre Erfahrung. Sie wurden am {birthDate} in {birthPlace} geboren.",
    "@basicPlaceholderContent": {
        "description": "Eine Nachricht mit den Parametern Name, Alter, Erfahrung, Geburtsdatum und Geburtsort",
        "placeholders": {
            "name": {
                "type": "String",
                "example": "Enes",
                "description": "Name des Benutzers"
            },
            "age": {
                "type": "int",
                "example": "23",
                "description": "Alter des Benutzers"
            },
            "experience": {
                "type": "double",
                "example": "2.5",
                "description": "Erfahrung des Benutzers"
            },
            "birthDate": {
                "type": "DateTime",
                "format": "yMMMEd",
                "example": "Tue, 10 Nov 2020"
            },
            "birthPlace": {
                "type": "String",
                "example": "Istanbul, Türkiye",
                "description": "Geburtsort des Benutzers"
            }
        }
    },


    "nKangaroosTitle": "Pluraltext",
    "nKangaroosContent": "{count, plural, =0{keine Kängurus} =1{1 Känguru} other{{count} Kängurus}}",
    "@nKangaroosContent": {
        "description": "Eine Pluralbotschaft für Kängurus",
        "placeholders": {
            "count": {
                "type": "num",
                "format": "compact",
                "example": "0",
                "description": "Anzahl der Kängurus"
            }
        }
    },


    "pronounTitle": "Wählen Sie Geschlechtstext aus",
    "pronounContent": "{gender, select, male{er} female{sie} other{sie}}",
    "@pronounContent": {
        "description": "Eine geschlechtsspezifische Botschaft",
        "placeholders": {
            "gender": {
                "type": "String",
                "example": "he"
            }
        }
    },


    "numberAndCurrenciesTitle": "Anzahl und Währungen",

    "simpleCurrencyContent": "Der Preis des Produkts beträgt {currency1}",
    "@simpleCurrencyContent": {
        "description": "Eine Nachricht mit einem formatierten int|double-Parameter",
        "placeholders": {
            "currency1": {
                "type": "double",
                "format": "simpleCurrency",
                "example": "1000.0",
                "description": "Preis des Produkts",
                "optionalParameters": {
                    "decimalDigits": 2
                }
            }
        }
    },

    "compactLongContent": "Die Bevölkerung der {country} beträgt {compactLong}",
    "@compactLongContent": {
        "description": "Eine Nachricht mit einem formatierten Double-Parameter",
        "placeholders": {
            "compactLong": {
                "type": "double",
                "format": "compactLong",
                "example": "1000.0",
                "description": "Bevölkerung des Landes"
            },
            "country": {
                "type": "String",
                "example": "Turkey",
                "description": "Name des Landes"
            }
        }
    },

    "percentContent": "Auf {country} entfallen etwa {percent} der Weltbevölkerung",
    "@percentContent": {
        "description": "Eine Nachricht mit einem formatierten Double-Parameter",
        "placeholders": {
            "percent": {
                "type": "double",
                "format": "decimalPercentPattern",
                "example": "1000.0"
            },
            "country": {
                "type": "String",
                "example": "Turkey",
                "description": "Name des Landes"
            }
        }
    }
